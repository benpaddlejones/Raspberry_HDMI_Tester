# Raspberry Pi HDMI Tester - pi-gen Build Configuration
IMG_NAME="RPi_HDMI_Tester_PiOS"

# Base OS release (bookworm is the base for the latest Raspberry Pi OS)
RELEASE="bookworm"

# Target hostname
TARGET_HOSTNAME="hdmi-tester"

# Default user credentials
FIRST_USER_NAME="pi"
FIRST_USER_PASS="raspberry"

# Disable SSH for hardened field image
ENABLE_SSH=0

# Stage list: Build up to our custom stage3
STAGE_LIST="stage0 stage1 stage2 stage3"

# Compression for the final image
DEPLOY_COMPRESSION="zip"
